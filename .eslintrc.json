{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "jest/globals": true
    },
  "extends": ["airbnb-base","plugin:jest/all"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["@typescript-eslint", "eslint-plugin-tsdoc","jest"],
  "rules": {
    "tsdoc/syntax": "warn"
  }
}
